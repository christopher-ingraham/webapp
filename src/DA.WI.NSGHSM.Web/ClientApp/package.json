{
    "name": "DA.WI.NSGHSM.Web",
    "version": "0.0.0",
    "scripts": {
        "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
        "build:ssr": "ng run DA.WI.NSGHSM.Web:server:dev",
        "dotnet-restore-api": "cd ../../DA.WI.NSGHSM.Api && dotnet restore",
        "e2e": "ng e2e",
        "gen:hrm-input-piece-module": "cd ./src/app/production/crud-hrm-input-piece && ng generate crud-schematics:crud-module hrmInputPiece",
        "gen:hrm-job-module": "cd ./src/app/production/crud-hrm-job && ng generate crud-schematics:crud-module hrmJob",
        "gen:produced-coil-module": "cd ./src/app/production/crud-produced-coil && ng generate crud-schematics:crud-module producedCoil",
        "gen:rep-hm-piece": "cd ./src/app/quality-control-system/crud-rep-hm-piece && ng generate crud-schematics:crud-module repHmPiece",
        "gen:rep-hm-piece-stat": "cd ./src/app/quality-control-system/crud-rep-hm-piece-stat && ng generate crud-schematics:crud-module repHmPieceStat",
        "gen:test-entity-module": "cd ./src/app/example/test-entity && ng generate crud-schematics:crud-module testEntity",
        "gen:used-setup-module": "cd ./src/app/production/crud-used-setup && ng generate crud-schematics:crud-module usedSetup",
        "lint": "ng lint",
        "ng": "ng",
        "start": "ng serve",
        "start-dev": "ng serve",
        "start-devauth": "ng serve --configuration=devAuth",
        "start-devauth-all": "concurrently --kill-others npm:start-devauth-service-*",
        "start-devauth-service-api": "cd ../../DA.WI.NSGHSM.Api && dotnet run --environment \"DevelopmentAuth\"",
        "start-devauth-service-ids": "cd ../../DA.WI.NSGHSM.IdentityServer && dotnet run --environment \"DevelopmentAuth\"",
        "start-devauth-service-web": "cd ../../DA.WI.NSGHSM.Web && dotnet run --environment \"DevelopmentAuth\"",
        "start-api": "cd ../../DA.WI.NSGHSM.Api && dotnet run",
        "start-identity-server": "cd ../../DA.WI.NSGHSM.IdentityServer && dotnet run",
        "start-easymock": "easymock -d ./mock",
        "start-ng-mock": "ng serve --proxy-config proxy-mock.conf.json",
        "start-web": "cd ../../DA.WI.NSGHSM.Web && dotnet run",
        "test": "ng test"
    },
    "private": true,
