{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/identityserver_.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=NSGHSMIdentityDb;Trusted_Connection=true;MultipleActiveResultSets=true",
    "Oracle": "Data Source=172.30.102.200:1521/orcl;User Id=NSGHMRT_CONFIG;Password=NSGHMRT_CONFIG;"
  },
  "JwtSettings": {
    "SecretKey": "ThisIsAVeryLongSecretKeyForJWTTokenGeneration123456789",
    "Issuer": "https://localhost:5001",
    "Audience": "NSGHSM-api",
    "ExpirationHours": 24
  },
  "IdentityServerType": "AspNetCore",
  "Endpoints": {
    "WebUrl": [
      "https://localhost:5002",
      "http://localhost:5012"
    ]
  },
  "IssuerUri": "https://localhost:5001",
  "PublicOrigin": "https://localhost:5001",
  "IsTestUserStoreEnabled": true,
  "SigningCredential": {
    "IsDeveloperSigningCredentialEnabled": true,
    "KeyFilePath": "",
    "KeyFilePassword": ""
  },
  "ADConnection": {
    "IsEnabled": false,
    "Domain": "",
    "ServiceUserName": "",
    "ServiceuserPassword": ""
  },
  "AllowedHosts": "*"
}